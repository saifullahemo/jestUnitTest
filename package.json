{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "rm -rf .next && next dev",
    "build": "rm -rf .next & next build",
    "build-test": "rm -rf .next && NODE_ENV=test next build",
    "clean-install": "rm -rf node_modules pnpm-lock.yaml yarn.lock bun.lockb package-lock.json && pnpm install",
    "clean-install-ry": "rm -rf node_modules pnpm-lock.yaml yarn.lock bun.lockb package-lock.json && yarn",
    "clean-install-rb": "rm -rf node_modules pnpm-lock.yaml yarn.lock bun.lockb package-lock.json && bun install",
    "clean-build": "rm -rf .next node_modules pnpm-lock.yaml yarn.lock bun.lockb package-lock.json && pnpm install && pnpm build",
    "clean-build-rb": "rm -rf .next node_modules pnpm-lock.yaml yarn.lock bun.lockb package-lock.json && bun install && bun run build",
    "start": "NODE_ENV=production next start -p 8080",
    "start-local": "NODE_ENV=production next start -p 3000",
    "start-test-local": "NODE_ENV=test next start -p 3000",
    "commit": "npx cz",
    "format": "prettier --write .",
    "lint": "next lint",
    "prepare": "husky install",
    "update-icon-names": "node ./app/components/library/Icon/icon-name-generator.mjs",
    "test": "jest",
    "test-with-output": "jest --json --outputFile=output.json && jest-stare --json output.json",
    "test:watch": "jest --watchAll"
  },
  "dependencies": {
    "jest-config": "^29.7.0",
    "jest-stare": "^2.5.1",
    "next": "14.0.4",
    "node-sass": "^9.0.0",
    "react": "^18",
    "react-dom": "^18",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.70.0"
  },
  "devDependencies": {
    "@aritslimited/commitlint": "^1.2.0",
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/eslint-plugin-query": "^5.17.7",
    "@tanstack/react-query-devtools": "^5.17.12",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/archiver": "^6.0.2",
    "@types/express": "^4.17.21",
    "@types/file-saver": "^2.0.7",
    "@types/formidable": "^3.4.5",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.10",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-facebook-login": "^4.1.11",
    "@types/react-test-renderer": "^18.0.7",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "autoprefixer": "^10.0.1",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^9.2.12",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
